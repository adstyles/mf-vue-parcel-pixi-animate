{
  "name": "vue-parcel-demo",
  "version": "1.0.0",
  "description": "A Vue demo including Code Splitting, Hot Reloading, ESLint, Vuex, Vue Router and Less.",
  "scripts": {
    "dev": "parcel ./public/index.html --open",
    "build": "parcel build ./public/index.html NODE_ENV=production --no-source-maps --public-url ./ --out-dir ./dist",
    "lint": "eslint --ext .js,.vue src"
  },
  "keywords": [
    "Vue",
    "Parcel",
    "bulma"
  ],
  "author": "SlaneYang",
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "^9.1.3",
    "babel-polyfill": "^6.26.0",
    "browserslist": "^4.18.1",
    "bulma": "^0.7.2",
    "caniuse-lite": "^1.0.30001285",
    "fabric": "^4.6.0",
    "fabric-browseronly": "^3.5.0",
    "fabric-history": "^1.6.0",
    "howler": "^2.2.3",
    "mic-recorder-to-mp3": "^2.2.2",
    "postcss-modules": "^1.3.2",
    "public": "^0.1.5",
    "tslib": "^2.3.1",
    "vue": "^2.5.17",
    "vue-hot-reload-api": "^2.3.0",
    "vue-pixi-wrapper": "^2.1.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^2.2.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-vue": "^2.0.2",
    "eslint": "^5.5.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "eslint-plugin-vue": "^4.7.1",
    "less": "^3.8.1",
    "node-sass": "^6.0.1",
    "parcel-bundler": "1.12.3",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "vue-template-compiler": "^2.5.17"
  },
  "staticFiles": {
    "staticPath": [{
      "staticPath": "json",
      "staticOutDir": "json/"
    }]
  }
}